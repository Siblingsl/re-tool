{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "shell:allow-execute",
    "shell:allow-open",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "java",
          "args": true,
          "sidecar": false
        },
        {
          "name": "mvn",
          "cmd": "mvn",
          "args": true
        },
        {
          "name": "mvn.cmd",
          "cmd": "mvn.cmd",
          "args": true
        }
      ]
    }
  ]
}